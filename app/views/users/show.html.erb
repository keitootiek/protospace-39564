<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <%= "#{@user}さんの情報" %>
      </h2>
      <table class="table">
        <tbody>
          <tr>
            <th class="table__col1">名前</th>
            <td class="table__col2"><%= link_to "#{@user}" %></td>
          </tr>
          <tr>
            <th class="table__col1">プロフィール</th>
            <td class="table__col2"><%= link_to @user_profile  %></td>
          </tr>
          <tr>
            <th class="table__col1">所属</th>
            <td class="table__col2"><%= link_to @user_occupation %></td>
          </tr>
          <tr>
            <th class="table__col1">役職</th>
            <td class="table__col2"><%= link_to @user_position %></td>
          </tr>
        </tbody>
      </table>
      <h2 class="page-heading">
        <%= render partial:"prototypes/prototype",collection:@prototype %>
      </h2>
      <div class="user__card">
      </div>
    </div>
  </div>
</div>